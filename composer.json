{
    "name": "markwalet/laravel-hashed-route",
    "description": "A Laravel package that replaces the default route model binding for a safer version.",
    "keywords": ["laravel", "hash", "url", "security", "codec"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Walet",
            "email": "mark.walet@gmail.com",
            "homepage": "https://markwalet.me",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "~7.4|8.*",
        "hashids/hashids": "3.*|4.*",
        "laravel/framework": "6.*|7.*|8.*",
        "jenssegers/optimus": "^0.2.3"
    },
    "require-dev": {
        "orchestra/testbench": "4.*|5.*|6.*",
        "phpunit/phpunit": "~9.3"
    },
    "autoload": {
        "psr-4": {
            "MarkWalet\\LaravelHashedRoute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarkWalet\\LaravelHashedRoute\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarkWalet\\LaravelHashedRoute\\HashedRouteServiceProvider"
            ]
        }
    }
}
